{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb/base"],
  "rules":{
    "no-console":"warn",
    "eol-last":"off",
    "linebreak-style":"off",
    "no-param-reassign": "off",
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "comma-dangle": "off",
    "quotes": "off",
    "no-plusplus":"off",
    "no-use-before-define":"off",
    "radix":"off"
  }
}